{
  "name": "timeflies",
  "private": true,
  "repository": "https://github.com/Chnapy/timeflies.git",
  "author": "Chnapy <richardhaddad@hotmail.fr>",
  "devDependencies": {
    "concurrently": "5.1.0",
    "typescript": "3.7.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "heroku-postbuild": "yarn workspace @timeflies/shared run build && concurrently \"yarn workspace @timeflies/back run build\" \"yarn workspace @timeflies/front run build\""
  },
  "cacheDirectories": [
    "node_modules",
    "packages/shared/node_modules",
    "packages/back/node_modules",
    "packages/front/node_modules"
  ]
}